local scripts = {
    "https://raw.githubusercontent.com/nxuxe/-/refs/heads/main/Alerts",
    "https://raw.githubusercontent.com/nxuxe/-/refs/heads/main/Fov-MouseWheel",
    "https://raw.githubusercontent.com/nxuxe/-/refs/heads/main/Toggle%20Speed",
    "https://raw.githubusercontent.com/nxuxe/-/refs/heads/main/PDS-Alert-Kit-Test",
    "https://raw.githubusercontent.com/nxuxe/-/refs/heads/main/Entities-box"
}

for _, url in pairs(scripts) do
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if not success then
        warn("Failed to load script:", url, err)
    end
end
